<h1>Pages#index</h1>
<p>Find me in app/views/pages/index.html.erb</p>
<% if @user.nil? %>
<p><%= link_to "Sign Up", new_user_path %> </p> or
<%= link_to "Sign In",  login_path %></p>
<% else %>
<p><%= link_to "Profile", user_path(@user.id) %> or
<p><%= link_to "Sign Out", user_path(@user.id), method: :delete %></p>
<% end %>
